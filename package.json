{
  "name": "samegamets",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.ts",
  "bin": {
    "samegamets": "app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smack0007/SameGameTS.git"
  },
  "keywords": [
    "nwjs",
    "game"
  ],
  "author": "smack0007",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smack0007/SameGameTS/issues"
  },
  "homepage": "https://github.com/smack0007/SameGameTS#readme",
  "dependencies": {
    "nw": "^0.47.3"
  },
  "devDependencies": {
    "@types/node": "^14.6.0",
    "@types/unzip": "^0.1.1",
    "browserify": "^16.5.2",
    "concurrently": "^5.3.0",
    "extract-zip": "^2.0.1",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "build": "npm run build-tsc && npm run build-browserify && npm run build-pack",
    "build-tsc": "tsc --strict --typeRoots node_modules/@types -t es2015 -m commonjs --inlineSourceMap --inlineSources src/main.ts",
    "build-browserify": "browserify --debug src/main.js -o bin/app.js",
    "build-pack": "tsc --strict --typeRoots node_modules/@types scripts/build.ts && node scripts/build.js",
    "dev": "concurrently \"npm:dev-tsc\" \"npm:dev-watchify\" \"npm:dev-nwjs\"",
    "dev-tsc": "tsc -w --strict --typeRoots node_modules/@types -t es2015 -m commonjs --inlineSourceMap --inlineSources src/main.ts",
    "dev-watchify": "watchify -v --debug src/main.js -o bin/app.js",
    "dev-nwjs": "nw bin",
    "test": "echo 'Test'"
  }
}
